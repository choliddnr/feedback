{
  "$schema": "https://unpkg.com/knip@5/schema.json",
  "entry": [
    "app/app.vue",
    "nuxt.config.ts",
    "app/pages/**/*.vue",
    "app/layouts/**/*.vue",
    "app/middleware/**/*.ts"
  ],
  "project": [
    "app/**/*.ts",
    "app/**/*.vue",
    "server/**/*.ts",
    "app/composables/**/*.ts",
    "app/utils/**/*.ts",
    "app/stores/**/*.ts",
    "!eslint.config.mjs"
  ],
  "ignore": [
    "node_modules",
    ".nuxt",
    "dist",
    "server/db/migrations"
  ],
  "ignoreDependencies": [
    "knip",
    "typescript"
  ],
  "vitest": false
}