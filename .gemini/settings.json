{
  "ide": {
    "hasSeenNudge": true,
    "enabled": true
  },
  "mcpServers": {
    "playwright": {
      "command": "npx",
      "args": ["@playwright/mcp@latest"]
    },
    "chrome-devtools": {
      "command": "npx",
      "args": ["chrome-devtools-mcp@latest"]
    },
    "nuxt-ui": {
      "httpUrl": "https://ui.nuxt.com/mcp"
    }
  },
  "security": {
    "auth": {
      "selectedType": "oauth-personal"
    }
  }
}
